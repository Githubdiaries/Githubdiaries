#include<stdio.h>
void main()
{
    int a[100] ,i,n;

    printf("Enter the size of array\n");
    scanf("%d" ,&n);
    for(i=0;i<n;i++)
    {
        printf("Enter the number \n");
        scanf("%d" ,&a[i]);
      
    }
    printf("Array is \n");
    for(i=0;i<n;i++)
    {
    printf("%d\t" ,a[i]);
     }
}
